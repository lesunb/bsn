<launch> 
    <node name="g4t1" pkg="component" type="g4t1" output="screen" />

    <param name="frequency" value="6" /> <!-- 1 Hz  -->

    <!-- operating voltage -->
    <param name="voltage" value="3" type="double" />
    <param name="min_voltage" value="1" type="double" />
    <param name="max_voltage" value="6" type="double" />

    
</launch>
